{
  "name": "深圳住房轮询",
  "description": "深圳住房轮询插件",
  "version": "1.0",
  "manifest_version": 2,
  "permissions": ["storage", "https://*/", "http://*/"],
  "background": {
    "scripts": ["js/background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "imgs/icon.png",
      "32": "imgs/icon.png",
      "48": "imgs/icon.png",
      "128": "imgs/icon.png"
    }
  },
  "options_page": "options.html",
  "icons": {
    "16": "imgs/icon.png",
    "32": "imgs/icon.png",
    "48": "imgs/icon.png",
    "128": "imgs/icon.png"
  }
}
